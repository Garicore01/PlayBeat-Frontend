{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@icons/*": [
        "src/assets/icons/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@/*": [
        "src/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}