---
import {SignIn,SignOut}  from "auth-astro/components";
import {getSession} from "auth-astro/server";
import { sendInfo } from "@/utils/googleAuth";

const session = await getSession(Astro.request);
//console.log(session);
if (session !== null) {
  //console.log(session);
  try {
    await sendInfo(session);
  } catch (error) {
    console.log(error);
  }
}
